{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "no-console": false,
    "variable-name": false,
    "no-var-requires": false,
    "ordered-imports": [
      true,
      {
        "grouped-imports": true
      }
    ],
    "object-literal-sort-keys": false
  }
}